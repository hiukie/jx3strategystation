<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="community" extends="default" namespace="/community">
        <action name="notice">
            <result>/WEB-INF/content/community/notice.jsp</result>
        </action>
        <action name="strategy" class="showStrategyAction">
            <result>/WEB-INF/content/community/strategy.jsp</result>
        </action>
        <action name="entertainment" class="showCommonAction">
            <result>/WEB-INF/content/community/entertainment.jsp</result>
        </action>
        <action name="transaction" class="showTradingAction">
            <result>/WEB-INF/content/community/transaction.jsp</result>
        </action>
        <action name="commitCommon">
            <result>/WEB-INF/content/article/article-commit-common.jsp</result>
        </action>
        <action name="commitStrategy">
            <result>/WEB-INF/content/article/article-commit-strategy.jsp</result>
        </action>
        <action name="commitTrading">
            <result>/WEB-INF/content/article/article-commit-trading.jsp</result>
        </action>
    </package>
</struts>